<%- include("partials/header") -%>

<% if (isLoggedIn === true) {%>
  <h1><%= user %></h1>
<%} else {%>
  <h1>Home</h1>
<%}%>
<p> <%= homeStartingContent %> </p>

<% posts.forEach(post => {  %>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 100) %> ...
    <a href="/posts/<%=post.title%>">Read More</a>
  </p>
<% }) %>

<%- include("partials/footer") -%>
